#!/opt/modules/i12g/python/3.5.2/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'wbuild==1.7.1','console_scripts','wbuild'
__requires__ = 'wbuild==1.7.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wbuild==1.7.1', 'console_scripts', 'wbuild')()
    )
