{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {

        "number":      { "type": "number" },
        "street_name": { "type": "string" },
        "street_type": { "type": "string",
                         "enum": ["Street", "Avenue", "Boulevard"]
                       }
    },
    "required": ["number", "street_type", "street_name"]
}
